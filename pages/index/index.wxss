/**index.wxss**/
.classify{
  height: 70rpx;
  background-color: #1296db;
  padding: 20rpx 0
}
.classify_case{
  background-color: #fff;
  width: 90%;
  margin: 0 auto;
}
.classify input{
  height: 60rpx;
  text-indent:25%;
  border-radius: 15px;
}
.classify icon{
  position: fixed;
  top: 32rpx;
  left: 150rpx;
}
/* 轮播图 */
swiper-item image{
  width: 100%;
  height: 100%;
}
/* 中间导航 */
.ulli{
  height: 250rpx;
}
.ulli_yuan{
  height: 150rpx;
  width: 150rpx;
  float: left;
  background: pink;
  margin: 30rpx 18rpx;
  border-radius: 50%;
}
.ulli_yuan image{
  height: 100rpx;
  width: 100rpx;
  margin: 25rpx;
}
.ulli_yuan text{
  margin-left: 45rpx
}
/* 4.0 首页楼层 */
.floor-head{
  background-color: #f4f4f4;
  padding-top:20rpx;
}

.floor-head-image{
  width: 100%;
  height: 59rpx;
}

.floor-body{
  padding: 15rpx;
  display: flex;
}

.floor-body-left-image{
  width: 232rpx;
  height: 386rpx;
}

.floor-body-right-image{
  width: 233rpx;
  height: 188rpx;
  margin-left: 12rpx;
  margin-bottom: 10rpx;
}

/* 5.0 返回顶部 */
.top{
  width: 90rpx;
  height: 90rpx;
  background-color: rgba(255,255,255,.7);
  border-radius: 50%;
  position: fixed;
  right:20rpx;
  bottom: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
